class employee():
    def __init__(self,name,age,salary):
        self.name = name
        self.age = age
        self.salary = salary
class childemployee1(employee):
    def __init__(self,name,age,salary):
        self.name = name
        self.age = age
        self.salary = salary

class childemployee2(employee):
    def __init__(self,name,age,salary):
        self.name = name
        self.age = age
        self.salary = salary
emp1 = employee('kiran',22,10000)
emp2 = employee('satya sai',23,20000)
